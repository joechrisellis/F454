Method Render(Graphics2D g)
	g.SetColor(Color)
	ScalingManager SM = GraphingEngine.GetScalingManager()
	
	Integer POINT_SIZE = 4 # 4 radius circle

	# we translate the points, not scale them
	Double XPosition = SM.GetTranslated(x) - (POINT_SIZE / 2)
	Double YPosition = SM.GetTranslated(y) - (POINT_SIZE / 2)

	g.FillEllipse(XPosition, YPosition, POINT_SIZE, POINT_SIZE)
