Method Render(Graphics2D g)
	g.SetColor(Color)
	ScalingManager SM = GraphingEngine.GetScalingManager()

	Double PrevX = 0;
	Double PrevY = 0;
	For(Double x = LBound; x <= UBound; x += GraphingEngine.GetResolution())

		Double NewY = f(x) # where f(x) is user inputted function

		Double[] Point1 = SM.GetTransformed(PrevX, PrevY)
		Double[] Point1 = SM.GetTransformed(x, NewY)

		g.DrawLineBetween(Point1, Point2)

		PrevX = x
		PrevY = NewY
