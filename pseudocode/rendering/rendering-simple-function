Method Render(Graphics2D g)
	g.SetColor(Color)
	
	Point Prev = Points[0]
	For(Point New in Points)
		Double[] Point1 = SM.GetTransformed(Prev.x, Prev.y)
		Double[] Point2 = SM.GetTransformed(New.x, New.y)
		g.DrawLineBetween(Point1, Point2)
		Prev = New

Method Initialise()
	For(Double x = LBound; x < UBound; x += ScalingManager.GetResolution())
		Points.add(new Point(x, f(x))) # where f(x) is the user inputted function
